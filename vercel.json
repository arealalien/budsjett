{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/**/*.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "/api/.*", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}