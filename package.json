  "scripts": {
    "css:watch": "sass --watch src/sass/main.scss:src/css/main.css",
    "css:compile": "sass src/sass/main.scss:src/css/main.css",
    "css:prefix": "postcss --use autoprefixer -b \"last 4 versions\" src/css/main.css -o src/css/main.css",
    "css:compress": "css-minify -f src/css/main.css -o src/css",
    "css:build": "npm-run-all css:compile css:prefix css:compress",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "prisma generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concat": "^1.0.3",
    "css-minify": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.89.2"
  }
